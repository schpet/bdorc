{
  "name": "@schpet/easy-bead-oven",
  "version": "0.2.0",
  "license": "ISC",
  "exports": "./main.ts",
  "tasks": {
    "run": "deno run --allow-run --allow-read main.ts",
    "test": "deno test --allow-run --allow-read --allow-write",
    "check": "deno check **/*.ts",
    "fmt": "deno fmt",
    "fmt:check": "deno fmt --check",
    "lint": "deno lint"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "@std/toml": "jsr:@std/toml@1",
    "@std/cli": "jsr:@std/cli@^1.0.12",
    "@std/fmt/colors": "jsr:@std/fmt@^1/colors",
    "@cliffy/command": "jsr:@cliffy/command@^1.0.0-rc.8",
    "@cliffy/prompt": "jsr:@cliffy/prompt@^1.0.0-rc.8"
  },
  "publish": {
    "include": [
      "main.ts",
      "src/",
      "README.md",
      "LICENSE"
    ]
  }
}
